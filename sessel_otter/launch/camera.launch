<launch>
  <node name="ar_cam" pkg="usb_cam" type="usb_cam_node" output="screen">
    <param name="pixel_format" value="yuyv"/>
    <param name="camera_name" value="ar_cam" />
    <param name="camera_frame_id" value="ar_cam" />
    <param name="video_device" value="/dev/video0" />

  </node>
</launch>
